<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="wrap_content"
  android:orientation="horizontal">
  <TextView
    android:id="@+id/tv_trial_list_group"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingTop="@dimen/listview_group_text_padding"
    android:paddingBottom="@dimen/listview_group_text_padding"
    android:paddingLeft="?android:attr/expandableListPreferredItemPaddingLeft"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    tools:ignore="RtlHardcoded,RtlSymmetry" />

  <ImageView
    android:id="@+id/iv_trial_list_block"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:padding="@dimen/listview_group_text_padding"
    android:src="@drawable/ic_baseline_block"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>